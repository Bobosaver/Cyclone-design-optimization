from pyfmi import load_fmu
import pandas as pd
import matplotlib.pyplot as plt

# 通过pyfmi读取fmu对象，将fmu文件放置于统一目录下
model = load_fmu(r'C:\Users\Yin\Desktop\Cyclone Seperator fengsu_lijing_shoujilv.fmu')
# 获取模型中的变量，并打印出来
print(model.get_model_variables())
# 将模型中的第三个值设置为200，也可以使用 model.set('A', 0.1) 的方式来设置变量的值，其中'A'是变量名
model.set_real([0], [15])
model.set_real([1], [0.000015])
res = model.simulate()
print(model.get('inlet.StaticPressure.average'))
print(model.get_real([0, 1, 2, 3, 4, 5, 6, 7]))
